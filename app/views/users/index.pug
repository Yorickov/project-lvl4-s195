extends /layouts/app.pug

block content
  .container
    .row.pt-4
      .col-12.mx-auto
        table.table.table-striped.table-hover.table-bordered
          caption users list
          thead.bg-secondary.text-white
            tr
              th #
              th Full Name
              th Email
              th Created at
          tbody
            each user in users
              tr
                td= user.id
                td= user.fullName
                td= user.email
                td= user.createdAt
