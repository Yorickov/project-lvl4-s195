{
  "name": "task-manager",
  "version": "0.2.1",
  "description": "task manager on Koa and PostgreSQL",
  "main": "dist/index.js",
  "bin": {
    "server": "dist/bin/index.js"
  },
  "engines": {
    "node": "8"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "gulp": "gulp",
    "sequelize": "sequelize",
    "nodemon": "NODE_ENV=development nodemon",
    "test": "NODE_ENV=test jest",
    "babel-node": "babel-node",
    "flow": "flow",
    "flow-typed": "flow-typed",
    "start": "NODE_ENV=production gulp production",
    "eslint": "eslint",
    "webpack": "webpack",
    "build": "npm run webpack -- -p --env production",
    "postinstall": "npm run build",
    "migrate": "NODE_ENV=development npm run sequelize db:migrate"
  },
  "jest": {
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/__tests__/__fixtures__/",
      "<rootDir>/__tests__/temp/"
    ],
    "verbose": true,
    "coverageReporters": [
      "lcov"
    ],
    "collectCoverage": true
  },
